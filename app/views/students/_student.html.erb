<article class="m-4">
  <table class="table table-sm">
    <thead class="table-dark">
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>E-mail</th>
        <th>Phone</th>
        <th>Course</th>
        <th>Grade</th>
      </tr>
    </thead>
    <tbody>
      <tr id="<%= dom_id student %>">
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.email %></td>
        <td><%= student.phone %></td>
        <td><%= student.course %></td>
        <td><%= student.grade %></td>
      </tr>
    </tbody>
  </table>
</article>
